{
  "packages": [
    {
      "package_info": {
        "name": "azure-cognitiveservices-personalizer",
        "install_type": "pypi",
        "prefer_source_distribution": false,
        "version": "==0.1.0"
      },
      "exclude_path": [
        "test*",
        "example*",
        "sample*",
        "doc*"
      ]
    }
  ],
  "target_repo": {
    "folder": "preview",
    "branch": "smoke-test",
    "url": "https://github.com/MicrosoftDocs/azure-docs-sdk-python"
  },
  "required_packages": [
    {
      "install_type": "pypi",
      "name": "azure-mgmt-core"
    },
    {
      "name": "azure-storage-blob",
      "version": "12.6.0b1",
      "install_type": "pypi"
    },
    {
      "install_type": "pypi",
      "name": "azure-core"
    },
    {
      "install_type": "pypi",
      "name": "aiohttp"
    }
  ],
  "run_post_toc_script": false
}
