{
  "target_repo": {
    "branch": "smoke-test",
    "url": "https://github.com/MicrosoftDocs/azure-docs-sdk-python",
    "folder": "previous"
  },
  "packages": [
    {
      "package_info": {
        "name": "azure-ai-metricsadvisor",
        "install_type": "pypi",
        "prefer_source_distribution": "true",
        "version": "==1.0.1"
      },
      "exclude_path": [
        "test*",
        "example*",
        "sample*",
        "doc*"
      ],
      "extension_config": {
        "autodoc_default_options": {
          "inherited-members": 1
        }
      }
    }
  ],
  "required_packages": [
    {
      "install_type": "pypi",
      "name": "msrestazure"
    }
  ],
  "run_post_toc_script": false
}
