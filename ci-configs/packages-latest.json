{
  "target_repo": {
    "url": "https://github.com/MicrosoftDocs/azure-docs-sdk-python",
    "branch": "smoke-test"
  },
  "packages": [
    {
      "package_info": {
        "name": "azure-iot-device",
        "install_type": "pypi",
        "version": "==3.0.0b2"
      },
      "exclude_path": [
        "test*",
        "example*",
        "sample*",
        "doc*"
      ]
    }
  ],
  "required_packages": [
    {
      "install_type": "pypi",
      "name": "pywin32-ctypes",
      "version": "0.0.1"
    },
    {
      "install_type": "pypi",
      "name": "requests",
      "version": "2.14.1"
    },
    {
      "install_type": "pypi",
      "name": "vcrpy"
    },
    {
      "install_type": "pypi",
      "name": "python-dateutil"
    },
    {
      "install_type": "pypi",
      "name": "certifi",
      "version": "2018.4.16"
    },
    {
      "install_type": "pypi",
      "name": "isodate"
    },
    {
      "install_type": "pypi",
      "name": "requests_oauthlib"
    },
    {
      "install_type": "pypi",
      "name": "adal",
      "version": "0.5.0"
    },
    {
      "install_type": "pypi",
      "name": "keyring",
      "version": "12.0.2"
    },
    {
      "install_type": "pypi",
      "name": "grpcio"
    },
    {
      "install_type": "pypi",
      "name": "aiohttp"
    }
  ],
  "run_post_toc_script": false
}
